{
  "name": "ember-power-select-typeahead",
  "version": "0.8.0",
  "description": "The extensible select-typeahead component built for ember",
  "keywords": [
    "ember-addon",
    "ember-power-select-addon",
    "ember-power-select",
    "typeahead"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cibernox/ember-power-select-typeahead.git"
  },
  "license": "MIT",
  "author": {
    "name": "Miguel Camba",
    "email": "miguel.camba@gmail.com"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "homepage": "http://www.ember-power-select.com",
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint --ext js,ts .",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "prepublishOnly": "ember ts:precompile",
    "postpublish": "ember ts:clean"
  },
  "files": [
    "addon/",
    "app/",
    "blueprints/",
    "addon-test-support/",
    "test-support/",
    "index.js",
    "vendor"
  ],
  "dependencies": {
    "ember-cli-babel": "^7.18.0",
    "ember-cli-htmlbars": "^4.2.3",
    "ember-cli-typescript": "^3.1.3",
    "ember-power-select": "^4.0.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^1.3.0",
    "@glimmer/component": "^1.0.0",
    "@glimmer/tracking": "^1.0.0",
    "@types/ember": "^3.1.2",
    "@types/ember-qunit": "^3.4.8",
    "@types/ember__test-helpers": "^0.7.10",
    "@types/qunit": "^2.9.1",
    "@types/rsvp": "^4.0.3",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^1.5.3",
    "ember-cli": "~3.17.0",
    "ember-cli-app-version": "^3.2.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sass": "^10.0.1",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-typescript-blueprints": "^3.0.0",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.2",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^7.0.0",
    "ember-source": "~3.17.0",
    "ember-source-channel-url": "^2.0.1",
    "ember-template-lint": "^2.4.0",
    "ember-try": "^1.4.0",
    "eslint": "^6.8.0",
    "eslint-plugin-ember": "^8.1.1",
    "eslint-plugin-node": "^11.0.0",
    "loader.js": "^4.2.3",
    "npm-run-all": "^4.1.5",
    "qunit-dom": "^1.1.0",
    "sass": "^1.26.3",
    "typescript": "~3.7.5"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
